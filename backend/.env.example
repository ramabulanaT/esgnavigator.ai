# Frontend (Next.js on Vercel)
API_URL=https://<your-railway>.up.railway.app
PROXY_TIMEOUT_MS=10000
PROXY_FORWARD_HEADERS=authorization,x-request-id

# Backend (Express on Railway)
TRUST_PROXY=1
ENABLE_HELMET_CSP=false
CORS_ORIGINS=https://www.esgnavigator.ai,https://esgnavigator-ai.vercel.app
ALLOW_VERCEL_PREVIEW=true
ENABLE_RATE_LIMIT=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_PER_WINDOW=120
JSON_LIMIT=512kb
